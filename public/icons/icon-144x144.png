<svg width="120" height="120" viewBox="0 0 120 120" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle with gradient -->
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1D4ED8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F1F5F9;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main circle background -->
  <circle cx="60" cy="60" r="55" fill="url(#logoGradient)" stroke="#1E40AF" stroke-width="2"/>
  
  <!-- Gear/Settings icon representing service -->
  <g transform="translate(60,60)">
    <!-- Main gear -->
    <circle cx="0" cy="0" r="20" fill="url(#iconGradient)"/>
    <circle cx="0" cy="0" r="8" fill="none" stroke="#3B82F6" stroke-width="2"/>
    
    <!-- Gear teeth -->
    <rect x="-3" y="-25" width="6" height="10" fill="url(#iconGradient)" rx="1"/>
    <rect x="-3" y="15" width="6" height="10" fill="url(#iconGradient)" rx="1"/>
    <rect x="15" y="-3" width="10" height="6" fill="url(#iconGradient)" rx="1"/>
    <rect x="-25" y="-3" width="10" height="6" fill="url(#iconGradient)" rx="1"/>
    
    <!-- Diagonal gear teeth -->
    <rect x="12" y="-20" width="6" height="8" fill="url(#iconGradient)" rx="1" transform="rotate(45)"/>
    <rect x="12" y="12" width="6" height="8" fill="url(#iconGradient)" rx="1" transform="rotate(45)"/>
    <rect x="-18" y="12" width="6" height="8" fill="url(#iconGradient)" rx="1" transform="rotate(45)"/>
    <rect x="-18" y="-20" width="6" height="8" fill="url(#iconGradient)" rx="1" transform="rotate(45)"/>
  </g>
  
  <!-- Small decorative elements -->
  <circle cx="25" cy="25" r="3" fill="#FFFFFF" opacity="0.7"/>
  <circle cx="95" cy="95" r="2" fill="#FFFFFF" opacity="0.5"/>
  <circle cx="95" cy="25" r="2" fill="#FFFFFF" opacity="0.6"/>
</svg>